{
    "$defs": {
        "ConditionFulfilledValue": {
            "description": "Possible values to describe the state of a condition\nin the condition_fulfilled attribute of the ConditionNodes.",
            "enum": [
                "FULFILLED",
                "UNFULFILLED",
                "UNKNOWN",
                "NEUTRAL"
            ],
            "title": "ConditionFulfilledValue",
            "type": "string"
        },
        "EvaluatedFormatConstraint": {
            "description": "This class is the base class of all evaluated format constraints. They are used in the context of the\nMussfeldpr√ºfung after the format constraints are evaluated to see if the format constraint expression is\nfulfilled or not.",
            "properties": {
                "error_message": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Error Message"
                },
                "format_constraint_fulfilled": {
                    "title": "Format Constraint Fulfilled",
                    "type": "boolean"
                }
            },
            "required": [
                "format_constraint_fulfilled"
            ],
            "title": "EvaluatedFormatConstraint",
            "type": "object"
        }
    },
    "description": "A class that holds the results of a full content evaluation (meaning all hints, requirement constraints and\nformat constraints have been evaluated)",
    "properties": {
        "format_constraints": {
            "additionalProperties": {
                "$ref": "#/$defs/EvaluatedFormatConstraint"
            },
            "title": "Format Constraints",
            "type": "object"
        },
        "hints": {
            "additionalProperties": {
                "anyOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "null"
                    }
                ]
            },
            "title": "Hints",
            "type": "object"
        },
        "id": {
            "anyOf": [
                {
                    "format": "uuid",
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ],
            "default": null,
            "title": "Id"
        },
        "packages": {
            "anyOf": [
                {
                    "patternProperties": {
                        "^\\d+P$": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                {
                    "type": "null"
                }
            ],
            "default": null,
            "title": "Packages"
        },
        "requirement_constraints": {
            "additionalProperties": {
                "$ref": "#/$defs/ConditionFulfilledValue"
            },
            "title": "Requirement Constraints",
            "type": "object"
        }
    },
    "required": [
        "format_constraints",
        "requirement_constraints"
    ],
    "title": "ContentEvaluationResult",
    "type": "object"
}